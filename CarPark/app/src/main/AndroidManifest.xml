<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="carpark.carpark">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_ap"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_ap"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".task_list_a">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".add_task"
            android:label="@string/add_task"
            android:parentActivityName=".task_list_a" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".task_list_a" />
        </activity>
    </application>

</manifest>